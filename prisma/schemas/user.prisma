model User {
  id                 Int       @id @default(autoincrement())
  name               String
  jobTitle           String?
  profileDescription String?
  email              String    @unique
  password           String
  remember           Boolean   @default(false)
  rememberToken      String?
  createdAt          DateTime  @default(now())
  updatedAt          DateTime? @updatedAt
  deletedAt          DateTime?

  @@map("users")
}
